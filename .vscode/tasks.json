{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: cl.exe build active file",
            "command": "cl.exe",
            "args": [
                "/Zi",
                "/EHsc",
                "/nologo",
                "/std:c++20",
                "/MD",
                "/I",
                "${workspaceFolder}/OS_EMULATOR/headers",
                "/Fe${workspaceFolder}/source/Main.exe",
                "${workspaceFolder}/OS_EMULATOR/source/*.cpp",
                "legacy_stdio_definitions.lib"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$msCompile"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}